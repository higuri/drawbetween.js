{
  "name": "drawbetween",
  "version": "0.0.17",
  "description": "Draw images and shapes on the specified line segment",
  "main": "dist/drawbetween_cjs.js",
  "module": "dist/drawbetween_esm.js",
  "unpkg": "dist/drawbetween_iife.js",
  "types": "dist/drawbetween.d.ts",
  "files": [
    "/src",
    "/dist"
  ],
  "keywords": [
    "drawbetween",
    "draw",
    "canvas",
    "line segment",
    "shape",
    "line",
    "image",
    "rect",
    "circle"
  ],
  "author": {
    "name": "Yusuke Higuchi",
    "email": "higuri36@gmail.com",
    "url": "https://github.com/higuri"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/higuri/drawbetween.js/issues"
  },
  "homepage": "https://github.com/higuri/drawbetween.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/higuri/drawbetween.js.git"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc",
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "_build:pack": "yarn pack --filename drawbetween.tgz",
    "build:pack": "run-s build _build:pack",
    "_build:sample": "browserify ./sample/main.js -o ./sample/bundle.js",
    "build:sample": "run-s build _build:sample",
    "_watch:sample": "watch 'npm run _build:sample' ./dist --wait=1",
    "_open:sample": "opn ./sample/index.html",
    "start:sample": "run-p build:watch _watch:sample _open:sample",
    "lint": "eslint --fix ./src/*.ts",
    "test": "node ./npm_scripts/test.js",
    "clean": "node ./npm_scripts/clean.js",
    "prepublishOnly": "run-s clean build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "browserify": "^16.2.3",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fs-extra": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^4.0.0",
    "prettier": "^1.16.4",
    "rollup": "^1.4.1",
    "rollup-plugin-typescript2": "^0.19.3",
    "typescript": "^3.3.3333",
    "watch": "^1.0.2"
  }
}
